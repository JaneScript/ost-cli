{
  "name": "ost-cli",
  "version": "1.0.1",
  "description": "A mixture CLI for scaffolding Javascript projects",
  "preferGlobal": true,
  "bin": {
    "ost": "./bin/ost",
    "ost-init": "./bin/ost-init",
    "ost-cli": "./bin/ost-init"
  },
  "scripts": {
    "test:stage": "rm -rf ./build && cross-env NODE_ENV=stage node ./bin/ost init",
    "test:local": "rm -rf ./build && cross-env NODE_ENV=local node ./bin/ost init",
    "test:single": "rm -rf ./build && cross-env NODE_ENV=local node ./bin/ost init my-app",
    "test:unit": ""
  },
  "author": "leotw",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "chalk": "^2.3.2",
    "commander": "^2.14.1",
    "compressing": "^1.4.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "cross-spawn-with-kill": "^1.0.0",
    "download-git-repo": "^1.0.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "inquirer": "^5.1.0",
    "latest-version": "^3.1.0",
    "metalsmith": "^2.3.0",
    "ora": "^2.0.0",
    "randexp": "^0.5.2",
    "readline": "^1.3.0",
    "semver": "^5.5.0",
    "shelljs": "^0.8.1",
    "urllib": "^2.31.3"
  },
  "engines": {
    "node": ">=8.3.0"
  }
}
